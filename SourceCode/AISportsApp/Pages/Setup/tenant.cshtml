<!-- Content Header (Page header) -->
<div class="content-header">
<div class="container-fluid">
<div class="row mb-2">
<div class="col-sm-6">
<h1 class="m-0">
Tenants
</h1>
</div>
<div class="col-sm-6">
<ol class="breadcrumb float-sm-right">
<li class="breadcrumb-item"><a href="~/index">Home</a></li>
<li class="breadcrumb-item active">Tenants</li>
</ol>
</div>
</div>
</div><!-- /.container-fluid -->
</div><!-- /.content-header -->
<!-- Main content -->
<div class="content">
<div class="container-fluid">
<div class="row">
<div class="col-lg-12">
<div class="card card-primary ">
<div class="card-header">
<h5 class="m-0">Tenants</h5>
</div>
<div class="card-body">
<table id = "tenant-table" class="table table-bordered table-striped table-hover">
<thead>
<tr>
<th> TenantId</th>
<th> TenantSecret</th>
<th> TenantName</th>
<th> SubscriptionStartDate</th>
<th> SubscriptionEndDate</th>
<th></th>
<th></th>
</tr>
</thead>
<tbody></tbody>
<tfoot>
<tr>
<th> TenantId</th>
<th> TenantSecret</th>
<th> TenantName</th>
<th> SubscriptionStartDate</th>
<th> SubscriptionEndDate</th>
<th></th>
<th></th>
</tr>
</tfoot>
</table>
</div>
</div>
</div>
</div>
<!-- /.row -->
</div><!-- /.container-fluid -->
</div>
<!-- Dialog HTML (hidden by default) -->
<div id="confirmDialog" title="Confirm Delete" style="display: none;">
    <p>Do you want to delete?</p>
</div>
<!-- /.content -->
<Environment include = "Development" >
<script src="~/js/customscript/tenant.js" asp-append-version="true"></script>
</environment>
<Environment exclude = "Development" >
<script src="~/js/customscript/tenant.min.js" asp-append-version="true"></script>
</environment>
