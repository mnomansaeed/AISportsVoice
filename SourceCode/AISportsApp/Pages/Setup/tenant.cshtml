@page
@Html.AntiForgeryToken()
@model AISportsApp.Pages.tenantModel
@{
ViewData["Title"] = "tenant";
}
<!-- DataTables -->
<link rel = "stylesheet" href="~/templatecontent/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css">
<link rel = "stylesheet" href="~/templatecontent/plugins/datatables-responsive/css/responsive.bootstrap4.min.css">
